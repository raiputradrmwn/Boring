{
  "expo": {
    "name": "Boring",
    "slug": "boring",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/boring.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#E9F1FC"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/boring.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.boring"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-asset",
        {
          "assets": [
            "./assets/logo.png",
            "./assets"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2436a550-cff0-4ea4-82a2-068329aaadff"
      }
    }
  }
}
